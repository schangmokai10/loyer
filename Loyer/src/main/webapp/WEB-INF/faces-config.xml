<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <application>
        <el-resolver>
            org.springframework.web.jsf.el.SpringBeanFacesELResolver
        </el-resolver>
    </application>
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>                                                        
            <from-outcome>accueil</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/adminLoyer.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule> 

    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>client</from-outcome>
            <to-view-id>/clientLoyer.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule> 
    
    
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>maison</from-outcome>
            <to-view-id>/maisonLoyer.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule> 
    
    
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>rappel</from-outcome>
            <to-view-id>/rappelLoyer.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule> 
    
    
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>location</from-outcome>
            <to-view-id>/locationLoyer.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule> 
    
     <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>paye</from-outcome>
            <to-view-id>/payeLoyer.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule> 
    
</faces-config>
